  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS += -ldl
  LDDEPS +=
  ALL_LDFLAGS += $(LDFLAGS) -L../../libs -shared -Wl,-soname=liblua53.so -s
  LINKCMD = $(CC) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
  define PREBUILDCMDS
  endef
