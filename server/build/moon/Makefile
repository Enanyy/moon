ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

.PHONY: clean prebuild prelink

ifeq ($(config),debug)
  RESCOMP = windres
  TARGETDIR = ../../bin/Debug
  TARGET = $(TARGETDIR)/moon-d
  OBJDIR = ../../obj/moon/_Debug
  DEFINES += -DDEBUG -DASIO_STANDALONE -DSOL_CHECK_ARGUMENTS -D_SILENCE_CXX17_ALLOCATOR_VOID_DEPRECATION_WARNING
  INCLUDES += -I../.. -I../../moon -I../../moon/core -I../../third -I../../third/lua53
  FORCE_INCLUDE +=
  ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -g -Wall
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -g -Wall -std=c++17
  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS += -llua53 -lrapidjson -laoi -lprotobuf -lmysql -lrvo2 -ldl -lpthread -lstdc++fs
  LDDEPS += ../../bin/Debug/liblua53.so ../../bin/Debug/librapidjson.a ../../bin/Debug/libaoi.so ../../bin/Debug/libprotobuf.so ../../bin/Debug/libmysql.so ../../bin/Debug/librvo2.so
  ALL_LDFLAGS += $(LDFLAGS) -L../../libs -L../../bin/Debug -Wl,-rpath,'$$ORIGIN' -Wl,-rpath=./
  LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
	@echo Running postbuild commands
	cp -rf ../../bin/Debug/moon-d ../../example
  endef
all: prebuild prelink $(TARGET)
	@:

endif

